<% if @message %>
  alert('<%= @message %>');
  jQuery('#modal_form_sm').modal('hide');
  jQuery('#set-password-button-' + <%= @gallery.id %>).html("<%= j render partial: "gallery_set_buttons", locals: {gallery: @gallery, project: @gallery.project}  %>");
<% else %>
  <% if @error_keys %>
    require(["/assets/ujs_error_handler"],function(){
        HandleError('gallery_password', <%= raw @error_keys %>, <%= raw @complete_errors %>);
    });
  <% end %>
<% end %>