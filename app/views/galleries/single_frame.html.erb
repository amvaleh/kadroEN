      <% case @frame.gallery.download_limit %>
      <% when 0 %>
      <%=image_tag @frame.file_address(false,"light") , id: "frame"%>
      <% else %>
      <% if @frame.downloaded %>
      <%=image_tag @frame.file_address(false,"light") , id: "frame"%>
      <% else %>
      <%=image_tag @frame.file_address(true,"") , id: "frame"%>
      <% end %>
      <% end %>
