<!DOCTYPE html>
<html>
<head>
 
    <% if Rails.env.production? %>
      <%= render 'shared/externals/gtag_header' %>
        <%= render 'shared/externals/ganalytics' %>
          <% end %>

  <title>
    <% if @title.present? %>
      <%= @title %>
    <% else %>
    Best Photography Locations
    <% end %>
    | ShootEmpire
  </title>
  <meta name="theme-color" content="RGB(41, 135, 249)" />

  <meta charset="UTF-8">
  <meta name="description" content="Best Shoot Locations <%ShootLocationType.all.each do|sht|%><%=sht.title %>ØŒ<%end%> with Navigations, Rules and prices ">
  <meta name="keywords" content="Shoot Location, Photography Location, Photo Location">
  <meta name="author" content="ShootEmpire">
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
  <META NAME="ROBOTS" CONTENT="INDEX, FOLLOW">

      <link rel="icon" type="image/png" href="/img/shootempire.png">
      <meta name="theme-color" content="RGB(41, 135, 249)" />


  <%= csrf_meta_tags %>
  <% if Rails.env.production? %>
    <%= stylesheet_link_tag   'shoot_locations', media: 'all', 'data-turbolinks-track': 'reload'%>
    <%= javascript_include_tag 'shoot_locations', 'data-turbolinks-track': 'reload' %>
  <% else %>
    <%= stylesheet_link_tag   'shoot_locations', media: 'all', 'data-turbolinks-track': 'reload'%>
    <%= javascript_include_tag 'shoot_locations', 'data-turbolinks-track': 'reload'  %>
  <% end %>
  <link rel="icon" type="image/png" href="/img/location_picker.png">
  <script type="text/javascript" src='https://maps.google.com/maps/api/js?key=AIzaSyBqAlo6eTQeSbkOn3xYIjJ4e1zr4ewrSxU&sensor=false&libraries=places'></script>
  <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
  <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
</head>

<%#= render "shoot_locations/header" %>
<%= render "public/shared/wordpress_header" %>

<body>

  <%= render 'shared/externals/gtag_body' %>
<%= yield %>
</body>



<% if not photographer_signed_in? %>
  <%= render "types/sections/footer" %>
    <% end %>

    
<%#= render "public/shared/section_of_call_request" , dest_url: create_call_request_path %>

<%#= render "types/sections/footer", :@shoot_type=>@shoot_type%>

<script>
  $(document).ready(function () {
    $('.img-replace').each(function (i, obj) {
      console.log(i);
      newUrl = $(this).attr('id');
      console.log(newUrl);
      $(this).attr('src', newUrl);
    });
  });
</script>
</html>
