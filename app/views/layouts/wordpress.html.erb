<!DOCTYPE html>
<html>
<%- flash.each do |name, msg| -%>
    <%= content_tag :div, msg,class: "alert alert-info text-center" , :id=> "flash_#{name}" if msg.is_a?(String) %>
        <%- end -%>

<%= yield %>

<% if not photographer_signed_in? %>
    <%= render "types/sections/footer"%>
<% end %>


<script src="https://kit.fontawesome.com/your-fontawesome-kit.js" crossorigin="anonymous"></script>

<script>
    $(document).ready(function() {
        $('.img-replace').each(function(i, obj) {
            console.log(i);
            newUrl = $(this).attr('id');
            console.log(newUrl);
            $(this).attr('src', newUrl);
        });
    });
</script>
</html>
