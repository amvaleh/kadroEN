    $('[data-toggle="in_studio_book_tooltip"]').tooltip({title: "ابتدا روی رزرو با این عکاس کلیک کنید، سپس دکمه میخواهم بروم به آتلیه را بزنید تا مکان پروژه بصورت خودکار در محل آتلیه عکاس ست شود."});
</script>
<script>
    handler2 = Gmaps.build('Google');
    handler2.buildMap({
        provider: {
            zoomOut: 13,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        }, internal: {id: 'map'}
    }, function () {
        markers2 = handler2.addMarkers([
            <% if @photographer.location.present? %>
            {
                "lat": <%=@photographer.location.working_lat%>,
                "lng": <%=@photographer.location.working_long%>,
                "picture": {
                    "url": "https://www.kadro.co/kadroMapMarker.png",
                    "width": 50,
                    "height": 50
                },
                infowindow: 'محل فعالیت <%= @photographer.display_name %>'
            },
            <% end %>
            <% if @shoot_location.present? %>
            {
                "lat": <%= @shoot_location.address.lattitude %>,
                "lng": <%= @shoot_location.address.longtitude %>,
                "picture": {
                    "url": "https://app.kadro.co/img/studio-logo.png",
                    "width": 50,
                    "height": 50
                },
                infowindow: 'آتلیه <%= @photographer.display_name %>'
            },
            <% end %>

        ]);
        handler2.bounds.extendWith(markers2);
        handler2.fitMapToBounds();
        handler2.getMap().setZoom(11);
    });
</script>
<script type="text/javascript">
    $("#more_comments").on('click', function () {
        hidden_feedbacks = $(".feedback_hidden");
        hidden_feedbacks.each(function(idx, fb) {
            if ( idx < 5 ){
                var row = $(fb);
                $(row).removeClass("hidden feedback_hidden");
            }
        });
        if ($(".feedback_hidden").length < 5 ){
            $(".feedback_hidden").removeClass("hidden");
            $(".feedback_hidden").removeClass("feedback_hidden");
            $(this).addClass("hidden");
        }
    });
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })


    $(".scroll-up").on('click', function () {
        $('html, body').animate({
            scrollTop: $("#scroll-up").offset().top
        }, 100);
    });
</script>


