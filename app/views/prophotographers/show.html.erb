<script>


    $(window).on('load', () => {
        let iOS = !!navigator.platform && /iPad|iPhone|iPod/.test(navigator.platform);
        if (iOS)
            $('[role="tablist"] a').each((i,e) => {
                if (!$(e).attr('href'))
                    $(e).attr('href', $(e).data('target'));
            })
    });

    function Scroll_to_expertise(id) {
        $('html, body').animate({
            scrollTop: $(id).offset().top
        }, 100);
    }

    function fill_number() {
        mobile_number = "<%=@photographer.mobile_number%>";
        console.log(mobile_number);
        $("#photographer_mobile").html("شماره موبایل: " + mobile_number.toString());
    }

    $("#photos").owlCarousel({
        navText: [],
        nav: true,
        rtl: true,
        responsiveClass: true,
        responsive: {
            100: {
                items: 1
            },
            300: {
                items: 2
            },
            800: {
                items: 3
            },
            1100: {
                items: 4
            }
        },
        smartSpeed: 400,
        autoplay: true,
        autoplaySpeed: 500
    });

    $(".photographer-profile").css("width", "200px");
    $(".photographer-profile").css("height", "200px");

    var first_time = true;

    $("#ph-profile").on('click', function () {
        <% if @photographer.photographer_attachments.any? %>
        $("#profile-modal").modal('toggle');
        $(".svg-class").css("animation", "none");
        $(".svg-class").css("stroke", "#bf2bf9");
        $(".svg-class").css("stroke-width", "3px");
        <% end %>
        setTimeout(function (e) {
            $(window).resize();
            if (first_time) {
                $('#js-grid-slider-thumbnail').cubeportfolio({
                    layoutMode: 'slider',
                    drag: true,
                    auto: true,
                    autoTimeout: 5000,
                    autoPauseOnHover: true,
                    showNavigation: false,
                    showPagination: false,
                    rewindNav: true,
                    scrollByPage: true,
                    gridAdjustment: 'responsive',
                    mediaQueries: [
                        {
                            width: 0,
                            cols: 1
                        }
                    ],
                    gapHorizontal: 10,
                    gapVertical: 10,
                    caption: '',
                    displayType: 'fadeIn',
                    displayTypeSpeed: 400,
                    plugins: {
